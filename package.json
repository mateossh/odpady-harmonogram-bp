{
  "name": "odpady-harmonogram-bp",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rm -rf .astro dist node_modules",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "oxlint -c .config/.oxlintrc.json --type-aware --type-check --deny-warnings --report-unused-disable-directives",
    "lint:fix": "oxlint -c .config/.oxlintrc.json --fix --type-aware --type-check --deny-warnings --report-unused-disable-directives",
    "coredns": "coredns -conf .config/Corefile",
    "caddy": "caddy run --config .config/Caddyfile",
    "format": "oxfmt -c .config/.oxfmtrc.json",
    "format:check": "oxfmt -c .config/.oxfmtrc.json --check"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.6",
    "@types/bun": "^1.3.5",
    "astro-og": "^0.3.1",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "oxlint-tsgolint": "^0.11.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/sitemap": "^3.6.0",
    "@fontsource-variable/geist": "^5.2.8",
    "@lucide/astro": "^0.562.0",
    "@tailwindcss/vite": "^4.1.18",
    "@yeskunall/astro-umami": "0.0.7",
    "@zade/vite-plugin-strip-html-comments": "npm:@jsr/zade__vite-plugin-strip-html-comments",
    "astro": "^5.16.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.3.0"
  },
  "packageManager": "bun@1.3.4",
  "engines": {
    "bun": "1.3.4"
  }
}
